{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System.Net.Http": "Warning",
      "Microsoft.SmartPlaces.Facilities": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Trace",
        "System.Net.Http": "Warning"
      }
    }
  },
  "AppInsightsConnectionString": "InstrumentationKey=<instrumentationKey>;IngestionEndpoint=<IngestionEndpoint>;LiveEndpoint=<LiveEndpoint>",
  "AzureDigitalTwinsEndpoint": "https://<azureDigitalTwinsInstanceName>.api.<regionShortCode>.digitaltwins.azure.net",
  "StorageAccountEndpoint": "https://<storageAccountName>.blob.core.windows.net",
  "KeyVaultEndpoint": "https://<keyVaultName>.vault.azure.net",

  "EventHubName": "telemetry",
  "EventHubBatchMaximumCount": 10000,
  "EventHubPrefetchCount": 40000,
  "EventHubMaxTimeWithoutBatchInSeconds": 15
}